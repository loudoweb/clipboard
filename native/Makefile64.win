all: clipboard.dll tmp.clipboard.h moveResult

moveResult: 
	mkdir .\..\library\Windows64 && move /Y clipboard.dll .\..\library\Windows64\clipboard.dll && move /Y clipboard.obj .\..\library\Windows64\clipboard.obj

clipboard.dll: clipboard.obj
	cl /LD clipboard.obj

clipboard.obj: clipboard.c
	cl /c clipboard.c
	
tmp.clipboard.h: clipboard.h
	copy clipboard.h tmp.clipboard.h

.PHONY: all
